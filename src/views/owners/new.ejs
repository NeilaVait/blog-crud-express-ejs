<%- include("../partials/head.ejs") %>
<%- include("../partials/nav.ejs") %>

<main role="main">

  <h1 class="mb-3">Add new owner</h1>

  <form id="create-post-form" action="/owners/new" method="POST">
    <div class="form-group">
      <label for="name">Name</label>
      <input required type="text" class="form-control" id="name" name="name" placeholder="Enter name">
    </div>
    <div class="form-group">
      <label for="email">Email</label>
      <input required type="text" class="form-control" id="email" name="email" placeholder="Enter email">
    </div>


    <button type="submit" class="btn btn-primary">Save owner</button>
  </form>

</main>

